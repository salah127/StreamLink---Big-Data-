{
  "name": "@streamlink/mobile",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "pnpm exec expo export --platform all --output-dir dist",
    "dev": "pnpm exec expo start --offline --clear",
    "start": "pnpm exec expo start --offline",
    "android": "pnpm exec expo start --offline --android",
    "ios": "pnpm exec expo start --offline --ios",
    "web": "pnpm exec expo start --offline --web",
    "lint": "eslint \"{src,app}/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "clean": "if exist .expo rmdir /s /q .expo"
  },
  "dependencies": {
    "@expo-google-fonts/press-start-2p": "^0.2.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@streamlink/ui": "workspace:*",
    "@tanstack/react-query": "^5.24.1",
    "expo": "~50.0.7",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-notifications": "^0.32.15",
    "expo-router": "~3.4.7",
    "expo-status-bar": "~1.11.1",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.10",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "jest-expo"
  }
}